{
  "name": "z-retailing",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "check": "bunx @biomejs/biome check .",
    "build": "remix build --sourcemap && sentry-upload-sourcemaps --org myownprojectbytoririm --project javascript-remix",
    "deploy": "wrangler deploy",
    "dev": "remix dev --manual -c \"bun start\"",
    "format": "bunx @biomejs/biome format . --write",
    "lint": "bunx @biomejs/biome lint .",
    "start": "wrangler dev ./build/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.1.3",
    "@prisma/client": "5.7.1",
    "@prisma/extension-accelerate": "^0.6.2",
    "@remix-run/cloudflare": "^2.4.1",
    "@remix-run/css-bundle": "^2.4.1",
    "@remix-run/react": "^2.4.1",
    "@sentry/remix": "^7.98.0",
    "@supabase/supabase-js": "^2.39.2",
    "dayjs": "^1.11.10",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.2",
    "@cloudflare/workers-types": "^4.20230518.0",
    "@remix-run/dev": "^2.4.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "daisyui": "latest",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.1.6",
    "wrangler": "3.8.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "bun prisma/seed.ts"
  }
}